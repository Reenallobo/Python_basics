    "def StrNr(Word):\n",
    "    myList=list(Word)\n",
    "\n",
    "    counter=0\n",
    "    print(len(myList))\n",
    "    while len(myList):\n",
    "        [item for item in myList if item in myList]\n",
    "        \n",
    "        counter+=1\n",
    "    return item,counter\n",
    "\n",
    "StrNr('ABA')\n"
   ]
